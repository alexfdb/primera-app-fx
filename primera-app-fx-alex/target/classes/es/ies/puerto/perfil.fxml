<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="TOP_CENTER" prefHeight="358.0" prefWidth="436.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.ies.puerto.controller.PerfilController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="60.0" />
    </padding>
   <children>
      <Text fx:id="nickText" strokeType="OUTSIDE" strokeWidth="0.0" text="nick" />
      <Text fx:id="contraseniaText" strokeType="OUTSIDE" strokeWidth="0.0" text="contrasenia" />
      <Text fx:id="nombreText" strokeType="OUTSIDE" strokeWidth="0.0" text="nombre" />
      <Text fx:id="emailText" strokeType="OUTSIDE" strokeWidth="0.0" text="email" />
      <Button fx:id="inicioButton" mnemonicParsing="false" onAction="#inicioButtonClick" text="Inicio">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Button>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
